issn = {1362-4962},
journal = {Nucleic acids research},
keywords = {Artificial Intelligence,Catalytic Domain,Internet,Peptide Synthases,Peptide Synthases: chemistry,Software,Substrate Specificity},
month = jul,
number = {Web Server issue},
pages = {W362--7},
pmid = {21558170},
title = {{NRPSpredictor2--a web server for predicting NRPS adenylation domain specificity.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3125756\&tool=pmcentrez\&rendertype=abstract},
volume = {39},
