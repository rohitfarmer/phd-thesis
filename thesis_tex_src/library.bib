journal = {The Journal of biological chemistry},
keywords = {Acetyl Coenzyme A,Acetyl Coenzyme A: metabolism,Animals,Birds,Chromatography,Cloning,Cytosol,Cytosol: enzymology,DEAE-Cellulose,Electrophoresis,Escherichia coli,Escherichia coli: genetics,Hydroxymethylglutaryl-CoA Synthase,Hydroxymethylglutaryl-CoA Synthase: genetics,Hydroxymethylglutaryl-CoA Synthase: isolation \& pu,Hydroxymethylglutaryl-CoA Synthase: metabolism,Isoenzymes,Isoenzymes: genetics,Isoenzymes: isolation \& purification,Isoenzymes: metabolism,Kinetics,Liver,Liver: enzymology,Magnesium,Magnesium: pharmacology,Mitochondria,Molecular,Mutagenesis,Polyacrylamide Gel,Recombinant Proteins,Recombinant Proteins: isolation \& purification,Recombinant Proteins: metabolism,Restriction Mapping,Site-Directed,Sulfhydryl Compounds,Sulfhydryl Compounds: metabolism},
