month = jun,
number = {25},
pages = {4064--92},
pmid = {16761306},
title = {{Biomolecular modeling: Goals, problems, perspectives.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16761306},
volume = {45},
year = {2006}
}
