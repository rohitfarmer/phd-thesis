keywords = {Acetylation,Animals,Binding Sites,Birds,Chromatography,Cysteamine,Cysteamine: pharmacology,Cysteine,Cysteine: analysis,Dithiothreitol,Dithiothreitol: pharmacology,Electrophoresis,Glutathione,Glutathione: pharmacology,Hydroxymethylglutaryl-CoA Synthase,Hydroxymethylglutaryl-CoA Synthase: metabolism,Ion Exchange,Kinetics,Liver,Liver: enzymology,Oxo-Acid-Lyases,Paper,Polyacrylamide Gel,Pronase,Protein Binding,Thin Layer,Time Factors},
