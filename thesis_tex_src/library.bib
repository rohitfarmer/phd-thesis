keywords = {Fast Atom Bombardment,Ketones,Ketones: chemical synthesis,Ketones: chemistry,Magnetic Resonance Spectroscopy,Mass,Molecular Structure,Multienzyme Complexes,Multienzyme Complexes: genetics,Multienzyme Complexes: metabolism,Recombinant Proteins,Recombinant Proteins: biosynthesis,Recombinant Proteins: metabolism,Secondary Ion,Spectrometry,Streptomyces,Streptomyces: enzymology},
