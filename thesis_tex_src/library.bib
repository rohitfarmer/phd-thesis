keywords = {Molecular Dynamics Simulation,Mutagenesis,Mutation,Protein Structure,Secondary,Site-Directed,alpha-Amylases,alpha-Amylases: chemistry,alpha-Amylases: genetics},
month = jan,
number = {9},
pages = {e24214},
pmid = {21915299},
title = {{Molecular dynamics of mesophilic-like mutants of a cold-adapted enzyme: insights into distal effects induced by the mutations.}},
