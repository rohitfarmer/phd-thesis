pages = {10569--75},
pmid = {8756713},
title = {{Fatty acid synthase: in vitro complementation of inactive mutants.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/8756713},
volume = {35},
year = {1996}
}
