volume = {230},
year = {1993}
}
@article{Zheng2011,
abstract = {The process by which ??-stereocenters of polyketide intermediates are set by modular polyketide synthases (PKSs) when condensation is not immediately followed by reduction is mysterious. However, the reductase-incompetent ketoreductase (KR) from the third module of 6-deoxyerythronolide B synthase has been proposed to operate as a racemase, aiding in the epimerization process that reverses the orientation of the ??-methyl group of the polyketide intermediate generated by the ketosynthase to the configuration observed in the 6-deoxyerythronolide B final product. To learn more about the epimerization process, the structure of the C2-type KR from the third module of the pikromycin synthase, analogous to the KR from the third module of 6-deoxyerythronolide B synthase, was determined to 1.88 ?? resolution. This first structural analysis of this KR-type reveals differences from reductase-competent KRs such as that the site NADPH binds to reductase-competent KRs is occluded by side chains and the putative catalytic tyrosine possesses more degrees of freedom. The active-site geometry may enable C2-type KRs to align the thioester and ??-keto groups of a polyketide intermediate to reduce the pK
