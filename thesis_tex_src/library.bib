title = {{Protein folding kinetics and thermodynamics from atomistic simulations.}},
url = {http://link.aps.org/doi/10.1103/PhysRevLett.96.238102 http://www.ncbi.nlm.nih.gov/pubmed/16803409},
volume = {96},
year = {2006}
}
