issn = {1476-4687},
journal = {Nature},
month = jun,
number = {7506},
pages = {512--7},
pmid = {24965652},
title = {{Structure of a modular polyketide synthase.}},
url = {http://www.nature.com/doifinder/10.1038/nature13423 http://www.ncbi.nlm.nih.gov/pubmed/24965652},
volume = {510},
year = {2014}
}
