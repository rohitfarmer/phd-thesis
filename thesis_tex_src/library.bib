issn = {0022-2836},
journal = {Journal of molecular biology},
keywords = {Amino Acid Sequence,Animals,Antibodies,Antibodies: metabolism,Binding Sites,Electron,Fatty Acid Synthases,Fatty Acid Synthases: metabolism,Guinea Pigs,Harderian Gland,Harderian Gland: metabolism,Hydrolysis,Immunoblotting,Lacrimal Apparatus,Lacrimal Apparatus: metabolism,Microscopy,Molecular Sequence Data},
month = sep,
number = {1},
pages = {183--95},
pmid = {3184185},
title = {{Structure of fatty acid synthetase from the Harderian gland of guinea pig. Proteolytic dissection and electron microscopic studies.}},
