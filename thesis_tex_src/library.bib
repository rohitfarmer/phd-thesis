issn = {0002-7863},
journal = {Journal of the American Chemical Society},
keywords = {Antigens,Bacterial Proteins,CD4,CD4: chemistry,Chemical,Escherichia coli Proteins,HIV Envelope Protein gp120,HIV Envelope Protein gp120: chemistry,Models,Molecular,Phosphoenolpyruvate Sugar Phosphotransferase Syste,Protein Structure,Proteins,Proteins: chemistry,Quaternary},
