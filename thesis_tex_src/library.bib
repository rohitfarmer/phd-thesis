journal = {Biochemistry},
keywords = {Amino Acid,Amino Acid Motifs,Amino Acid Sequence,Anti-Bacterial Agents,Anti-Bacterial Agents: biosynthesis,Binding Sites,Catalysis,Crystallization,Crystallography,Dimerization,Erythromycin,Erythromycin: analogs \& derivatives,Erythromycin: biosynthesis,Hydrogen-Ion Concentration,Macrolides,Molecular Sequence Data,Multienzyme Complexes,Multienzyme Complexes: chemistry,Protein Structure,Sequence Alignment,Sequence Homology,Structure-Activity Relationship,Substrate Specificity,Tertiary,Thiolester Hydrolases,Thiolester Hydrolases: antagonists \& inhibitors,Thiolester Hydrolases: chemistry,X-Ray},
