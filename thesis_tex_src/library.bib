issn = {0006-2960},
journal = {Biochemistry},
keywords = {3-Oxoacyl-(Acyl-Carrier-Protein) Synthase,3-Oxoacyl-(Acyl-Carrier-Protein) Synthase: antagon,3-Oxoacyl-(Acyl-Carrier-Protein) Synthase: chemist,3-Oxoacyl-(Acyl-Carrier-Protein) Synthase: genetic,Acyl Coenzyme A,Acyl Coenzyme A: biosynthesis,Animals,Bicarbonates,Bicarbonates: chemistry,Binding Sites,Binding Sites: genetics,Catalysis,Chickens,Cysteine,Cysteine: chemistry,Cysteine: genetics,Decarboxylation,Enzyme Inhibitors,Enzyme Inhibitors: chemistry,Fatty Acid Synthetase Complex,Fatty Acid Synthetase Complex: chemistry,Fatty Acid Synthetase Complex: genetics,Humans,Iodoacetamide,Iodoacetamide: chemistry,Malonyl Coenzyme A,Malonyl Coenzyme A: chemistry,Mutagenesis,Protein Structure,Rats,Site-Directed,Substrate Specificity,Substrate Specificity: genetics,Tertiary,Tertiary: genetics},
