issn = {1477-4054},
journal = {Briefings in bioinformatics},
keywords = {Coevolution,Computer Simulation,Evolution,Molecular,Mutual information,Phylogeny,RNA,ROC curves,Ribosomal,Ribosomal: genetics,Sequence Alignment,Structure prediction,coevolution,mutual information,phylogeny,roc curves,structure prediction},
month = mar,
