keywords = {Amino Acid Isomerases,Amino Acid Isomerases: metabolism,Amino Acid Sequence,Base Sequence,Chromatography,Cloning,DNA Primers,Gramicidin,Gramicidin: biosynthesis,Gramicidin: chemistry,High Pressure Liquid,Mass Spectrometry,Molecular,Molecular Sequence Data,Mutagenesis,Peptide Synthases,Peptide Synthases: metabolism,Site-Directed,Tyrocidine,Tyrocidine: biosynthesis,Tyrocidine: chemistry},
month = jan,
number = {2},
pages = {620--9},
pmid = {11856321},
title = {{Mutational analysis of the C-domain in nonribosomal peptide synthesis.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/11856321},
volume = {269},
