doi = {10.1002/anie.201308176},
file = {:home/rxf126/Documents/Mendeley/Sugimoto et al. - 2014 - Rational Design of Modular Polyketide Synthases Morphing the Aureothin Pathway into a Luteoreticulin Assembly L.pdf:pdf},
