issn = {1439-4227},
journal = {Chembiochem : a European journal of chemical biology},
keywords = {Amino Acid Sequence,Animals,Beetles,Beetles: enzymology,Beetles: genetics,Catalysis,Cosmids,Cosmids: genetics,DNA,DNA: biosynthesis,DNA: genetics,Enzymologic,Enzymologic: genetics,Gene Expression Regulation,Gene Library,Molecular Sequence Data,Multienzyme Complexes,Multienzyme Complexes: chemistry,Multienzyme Complexes: genetics,Multienzyme Complexes: metabolism,Pseudomonas aeruginosa,Pseudomonas aeruginosa: enzymology,Pseudomonas aeruginosa: metabolism,Pyrans,Pyrans: metabolism,Reverse Transcriptase Polymerase Chain Reaction},
