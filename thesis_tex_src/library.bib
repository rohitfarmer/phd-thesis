publisher = {Elsevier Ltd},
title = {{Employing modular polyketide synthase ketoreductases as biocatalysts in the preparative chemoenzymatic syntheses of diketide chiral building blocks}},
url = {http://dx.doi.org/10.1016/j.chembiol.2011.07.021},
