keywords = {Algorithms,Amino Acid Sequence,Biopolymers,Biopolymers: chemistry,Computer Simulation,Models,Molecular,Monte Carlo Method,Oligopeptides,Oligopeptides: chemistry,Protein Conformation,Solutions,Thermodynamics,Water,generalized-ensemble algorithm,multicanonical algorithm,parallel tempering,protein folding,replica-exchange method,simulated tempering},
month = jan,
