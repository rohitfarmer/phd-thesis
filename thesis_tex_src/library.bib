file = {:home/rxf126/Documents/Mendeley/Winn et al. - 2005 - Issues in high-throughput comparative modelling a case study using the ubiquitin E2 conjugating enzymes.pdf:pdf},
issn = {1097-0134},
journal = {Proteins},
keywords = {Amino Acid Motifs,Amino Acid Sequence,Automation,Computational Biology,Computational Biology: methods,Crystallography, X-Ray,Databases, Protein,Models, Biological,Models, Molecular,Models, Theoretical,Molecular Sequence Data,Proline,Proline: chemistry,Protein Conformation,Protein Structure, Tertiary,Proteomics,Proteomics: methods,Quality Control,Sequence Analysis, Protein,Software,Static Electricity,Structural Homology, Protein,Temperature,Ubiquitin-Conjugating Enzymes,Ubiquitin-Conjugating Enzymes: chemistry},
