issn = {1097-0134},
journal = {Proteins},
keywords = {Binding Sites,Dimerization,Evolution,Models,Molecular,Protein Conformation,Proteins,Proteins: chemistry,Proteomics: methods,Structural Homology,c 2007 wiley-liss,cell signaling and protein,inc,protein,protein interaction,proteomics,structural},
month = may,
number = {2},
pages = {400--417},
pmid = {17299750},
title = {{PIER: protein interface recognition for structural proteomics.}},
