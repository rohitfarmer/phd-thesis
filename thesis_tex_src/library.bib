@article{Wang2004,
abstract = {We describe here a general Amber force field (GAFF) for organic molecules.$\backslash$nGAFF is designed to be compatible with existing Amber force fields$\backslash$nfor proteins and nucleic acids, and has parameters for most organic$\backslash$nand pharmaceutical molecules that are composed of H, C, N, O, S,$\backslash$nP, and halogens. It uses a simple functional form and a limited number$\backslash$nof atom types, but incorporates both empirical and heuristic models$\backslash$nto estimate force constants and partial atomic charges. The performance$\backslash$nof GAFF in test cases is encouraging. In test I, 74 crystallographic$\backslash$nstructures were compared to GAFF minimized structures, with a root-mean-square$\backslash$ndisplacement of 0.26 Angstrom, which is comparable to that of the$\backslash$nTripos 5.2 force field (0.25 Angstrom) and better than those of MMFF$\backslash$n94 and CHARMm (0.47 and 0.44 Angstrom, respectively). In test II,$\backslash$ngas phase minimizations were performed on 22 nucleic acid base pairs,$\backslash$nand the minimized structures and intermolecular energies were compared$\backslash$nto MP2/6-31G* results. The RMS of displacements and relative energies$\backslash$nwere 0.25 A and 1.2 kcal/mol, respectively. These data are comparable$\backslash$nto results from Parm99/RESP (0.16 Angstrom and 1.18 kcal/mol. respectively),$\backslash$nwhich were parameterized to these base pairs. Test III looked at$\backslash$nthe relative energies of 71 conformational pairs that were used in$\backslash$ndevelopment of the Parm99 force field. The RMS error in relative$\backslash$nenergies (compared to experiment) is about 0.5 kcal/mol. GAFF can$\backslash$nbe applied to wide range of molecules in an automatic fashion. making$\backslash$nit suitable for rational drug design and database searching. (C)$\backslash$n2004 Wiley Periodicals, Inc.},
author = {Wang, J M and Wolf, R M and Caldwell, J W and Kollman, P A and Case, D A},
doi = {10.1002/jcc.20035},
file = {:home/rxf126/Documents/Mendeley/Wang et al. - 2004 - Development and testing of a general amber force field.pdf:pdf},
isbn = {0192-8651},
journal = {J. Comput. Chem.},
keywords = {additive force field,force field parameterization,general amber force field,restrained electrostatic},
pages = {1157--1174},
pmid = {221624200007},
title = {{Development and testing of a general amber force field}},
volume = {25},
year = {2004}
}
@article{Broadhurst2003,
abstract = {Polyketides from actinomycete bacteria provide the basis for many valuable medicines, so engineering genes for their biosynthesis to produce variant molecules holds promise for drug discovery. The modular polyketide synthases are particularly amenable to this approach, because each cycle of chain extension is catalyzed by a different module of enzymes, and the modules are arranged within giant multienzyme subunits in the order in which they act. Protein-protein interactions between terminal docking domains of successive multienzymes promote their correct positioning within the assembly line, but because the overall complex is not stable in vitro, the key interactions have not been identified. We present here the NMR solution structure of a 120 residue polypeptide representing a typical pair of such domains, fused at their respective C and N termini: it adopts a stable dimeric structure which reveals the detailed role of these (predominantly helical) domains in docking and dimerization by modular polyketide synthases.},
